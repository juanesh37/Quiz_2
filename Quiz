
CREATE TABLE SALARIES (
    ID NUMBER(*,0) PRIMARY KEY,
    EMPLOYEE_ID NUMBER(*,0) NOT NULL,
    PROJECT_ID NUMBER (*,0) NOT NULL,
    SALARY NUMBER(10,2),
    CONSTRAINT FK_SALARY_EMPLOYEE FOREIGN KEY (EMPLOYEE_ID) REFERENCES SALARY_EMPLOYE(EMPLOYEE_ID),
    CONSTRAINT FK_SALARY_PROJECT FOREIGN KEY (PROJECT_ID) REFERENCES SALARY_PROJECT(PROJECT_ID)
    );
    
CREATE TABLE EMPLOYEES (
    ID NUMBER(*,0) PRIMARY KEY,
    MANAGER_ID NUMBER(*,0) NOT NULL,
    DATE_OF_JOINING DATE NOT NULL,
    FULL_NAME VARCHAR2(255 BYTE) NOT NULL,
    CONSTRAINT FK_MANAGER FOREIGN KEY (MANAGER_ID) REFERENCES MANAGER(MANAGER_ID)
    );
    
CREATE TABLE PROJECTS (
    ID NUMBER(*,0) PRIMARY KEY,
    CODE VARCHAR2(255 BYTE) NOT NULL,
    MANAGER_ID NUMBER(*,0),
    BUDGET NUMBER (10,2) NOT NULL,
    DATE_OF_BEGINNING DATE,
    DATE_OF_ENDING DATE,
    CONSTRAINT FK_MANAGER_PROJECT FOREIGN KEY (MANAGER_ID) REFERENCES MANAGER(MANAGER_ID)
    );

--QUERY 1

SELECT * FROM WHERE PROJECT_ID ='44905736599' COUNT(PROJECT_ID);

